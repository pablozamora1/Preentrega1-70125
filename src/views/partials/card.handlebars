<div class="homeCards">

    {{#each products}}
    <div class="cardProducts">
        <div class="imgCard">
            <img src={{this.thumbnails}} alt="">
        </div>
        <div>
            <h3>Titulo: {{this.title}}</h3>
            <p>Descripci√≥n: {{this.description}}</p>
            <p>Precio: {{this.price}}</p>
            <p>Stock: {{this.stock}}</p>
            <p>Categoria: {{this.category}}</p>
            <button type="submit">agregar al carrito</button>
        </div>
    </div>
    {{/each}}



</div>

{{#if hasNextPage}}
<a href="/?page={{nextPage}}">Siguiente</a>
{{/if }}
<p>pagina {{page}} de {{totalPages}}</p>

{{#if hasPrevPage}}
<a href="/?page={{prevPage}}">anterior</a>
{{/if }}